<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".aboutmepage">

    <TextView
        android:id="@+id/digitaltittle"
        android:layout_width="441dp"
        android:layout_height="68dp"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="203dp"
        android:background="#F8F3FA"
        android:padding="10dp"
        android:text="         Digital Skills"
        android:textAlignment="textStart"
        android:textAllCaps="true"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.078"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/digitalback"
        android:layout_width="55dp"
        android:layout_height="36dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="373dp"
        android:background="@drawable/ic_baseline_arrow_back_24"
        app:layout_constraintBottom_toBottomOf="@+id/digitaltittle"
        app:layout_constraintEnd_toEndOf="@+id/digitaltittle"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.111"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/digitalskillstext"
        android:layout_width="408dp"
        android:layout_height="776dp"
        android:layout_marginBottom="2dp"
        android:padding="15dp"
        android:text="@string/digitalskills"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/digitaltittle" />

</androidx.constraintlayout.widget.ConstraintLayout>
