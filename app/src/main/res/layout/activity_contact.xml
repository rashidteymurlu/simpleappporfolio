<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".aboutmepage">

    <Button
        android:id="@+id/contactback"
        android:layout_width="55dp"
        android:layout_height="36dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="373dp"
        android:background="@drawable/ic_baseline_arrow_back_24"
        app:layout_constraintBottom_toBottomOf="@+id/contacttitle"
        app:layout_constraintEnd_toEndOf="@+id/contacttitle"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.111"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/contacttitle"
        android:layout_width="441dp"
        android:layout_height="68dp"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="203dp"
        android:background="#F8F3FA"
        android:padding="12dp"
        android:text="         Contact"
        android:textAlignment="textStart"
        android:textAllCaps="true"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.078"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/phonenum"
        android:layout_width="410dp"
        android:layout_height="56dp"
        android:layout_marginTop="23dp"
        android:background="@color/white"
        android:padding="15dp"
        android:paddingStart="55dp"
        android:text="@string/callnumber"
        android:textAlignment="center"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.59"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contacttitle" />

    <TextView
        android:id="@+id/phonenum3"
        android:layout_width="410dp"
        android:layout_height="56dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="11dp"
        android:padding="15dp"
        android:paddingStart="70dp"
        android:text="@string/gmailaccount"
        android:textAlignment="center"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phonenum4" />

    <TextView
        android:id="@+id/phonenum4"
        android:layout_width="410dp"
        android:layout_height="56dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="11dp"
        android:padding="5dp"
        android:paddingStart="70dp"
        android:paddingTop="100dp"
        android:paddingEnd="50dp"
        android:text="@string/linkedinaccount"
        android:textAlignment="textStart"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phonenum" />

    <TextView
        android:id="@+id/phonenum2"
        android:layout_width="410dp"
        android:layout_height="56dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="11dp"
        android:padding="10dp"
        android:paddingStart="70dp"
        android:paddingEnd="20dp"
        android:text="@string/location"
        android:textAlignment="textStart"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phonenum3" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="46dp"
        android:layout_height="40dp"
        android:layout_marginTop="328dp"
        android:layout_marginEnd="-60dp"
        app:layout_constraintEnd_toStartOf="@+id/phonenum2"
        app:layout_constraintHorizontal_bias="0.888"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_location_on_24" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="52dp"
        android:layout_height="40dp"
        android:layout_marginEnd="-62dp"
        app:layout_constraintBottom_toBottomOf="@+id/phonenum3"
        app:layout_constraintEnd_toStartOf="@+id/phonenum3"
        app:layout_constraintTop_toTopOf="@+id/phonenum3"
        app:srcCompat="@drawable/ic_baseline_mail_outline_24" />

    <ImageView
        android:id="@+id/linkedinlogo"
        android:layout_width="41dp"
        android:layout_height="34dp"
        android:layout_marginStart="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/phonenum4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/phonenum4"
        app:srcCompat="@drawable/linkedinicon" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="42dp"
        android:layout_height="35dp"
        android:layout_marginStart="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/phonenum"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/phonenum"
        app:srcCompat="@drawable/contactme" />

</androidx.constraintlayout.widget.ConstraintLayout>
